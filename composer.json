{
    "name": "axytos/financial-services-api-sdk",
    "version": "1.1.0",
    "authors": [
        {
            "name": "axytos GmbH",
            "email": "info@axytos.com",
            "homepage": "https://www.axytos.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Axytos\\FinancialServices\\": "src/"
        },
        "files": [ "src/openapi_client_include.php" ]
    },
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-phpunit": "^1.1",
        "squizlabs/php_codesniffer": "3.*",
        "humbug/php-scoper": "^0.17.5"
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpstan"
        ],
        "generate-openapi-client": "@php ./scripts/generate-openapi-client.php ./openapi-generator.json ./openapi-generator-workspace ./openapi-client",
        "php-scoper": "vendor/bin/php-scoper",
        "phpunit": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf"
    }
}
